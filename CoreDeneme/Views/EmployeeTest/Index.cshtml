@model List<CoreDeneme.Controllers.Class1>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h1>Index</h1>

<table class="table table-bordered">
    <tr>

        <th>#</th>
        <th>Adı Soyadı</th>
        <th>Sil </th>
        <th>Düzenle </th>
    </tr>

    @foreach (var x in Model)
    {
        <tr>

            <td>@x.ID</td>
            <td>@x.Name</td>
            <td><a href="#" class="btn btn-outline-danger">Sil</a></td>
            <td><a href="#" class="btn btn-outline-success">Düzenle</a></td>
        </tr>
    }


</table>
<a href="/EmployeeTest/AddEmployee/" class="btn btn-outline-primary">Yeni Çalışan Girişi</a>